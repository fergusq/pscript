extern Void printf(Str s, Int i);

Void printInt(Int i) { printf("%d\n", i); }

model Printable {
	Void print();
}

extend Int with Printable {
	Void print() {
		printInt(this);
	}
}

Void shout(Printable p) {
	p.print();
	p.print();
	p.print();
}

Void shoutAll(Printable[] ps) {
	for (p in ps) shout(p);
}

Void main() {
	var arr = [1, 2, 3, 4];
	shoutAll(arr);
}
