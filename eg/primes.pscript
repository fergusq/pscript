model primes;

Void main() {	
	var primes = new ArrayList<Int> { [2] };
	var i = 3;
	var t = clock();
	while (true) {
		var isPrime = true;
		for (prime in primes.elements) {
			if (i % prime == 0) isPrime = false;
		}
		if (isPrime) {
			primes.add(i);
			var t2 = clock();
			println("" + i + "\t" + primes.size() + "\t" + (t2-t));
			t = t2;
		}
		i = i + 1;
	}
}

Void println(String str) puts(str.toString());

extern Void puts(Str str);
extern Int clock();
